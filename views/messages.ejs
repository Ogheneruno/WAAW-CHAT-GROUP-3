<% if (success_message && success_message.length > 0) { %>
    <div class="alert alert-success alert-success just-go" role="alert">
        <%= success_message %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>

    </div>
<% } %>


<% if (error_message && error_message.length > 0) { %>
    <div class="alert alert-danger alert-error just-go" role="alert">
        <%= error_message %>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
    </div>
<% } %>


<script>
    setTimeout( function () {
      var x = document.getElementsByClassName("alert-primary");
      for (var i = 0; i < x.length; i++) {
        x[i].style.display = "none";
      }
    }, 10000)
</script>
    